<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <title> Node App </title>

    <link rel="stylesheet" href="/styles.css">
  </head>
  <body>
    <h1> Pokemon </h1>

    <ul>
      <% pokemonList.forEach(function (aPokemon) { %>
        <li>
          <%= aPokemon.name %>
          - <%= aPokemon.types %>
        </li>
      <% }); %>
    </ul>
  </body>
</html>
